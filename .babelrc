{
  "presets": ["next/babel"],
  "plugins": [
    ["styled-components", { "ssr": true }],
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "@common": "./src/common",
          "@components": "./src/components",
          "@containers": "./src/containers",
          "@providers": "./src/providers",
          "@src": "./src"
        }
      }
    ]
  ]
}
